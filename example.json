{
	"globalCfg": {
		"blacklist": ["what", "eww", "gross"],
		"rootDir": "./downloads",
		"simultaneousDownloads": 12,
		"defaults" : {
			"type": "search",
			"url": "https://example.com",
			"auth": {
				"username": "username",
				"password": "password or API key (site dependent)"
			},
			"minsize": {
				"width": 1280,
				"height": 720
			},
			"ratio": "16:9",
			"minscore": 25,
			"excludeFileTypes": ["swf"],
			"ignoreBlacklist": false,
			"stopEarly": false,
			"moveNonmatching": false,
			"deleteNonmatching": false
		}
	},
	"sets": [
		{
			"downloadDir": "cool nintendo wallpapers",
			"search": ["nintendo"],
			"exclude": ["sonic_the_hedgehog"]
		}
	]
}
